{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-multi-assign": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "plugins": [
    "react"
  ]
}
